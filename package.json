{
  "name": "moleculer-postgesql-boilerplate",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services/**/*.service.js",
    "start": "moleculer-runner",
    "cli": "moleculer connect ",
    "ci": "jest --watch",
    "dc:up": "docker-compose --env-file=docker-compose.env up -d",
    "dc:logs": "docker-compose  --env-file=docker-compose.env logs",
    "dc:dn": "docker-compose --env-file=docker-compose.env down -v"
  },
  "keywords": [
    "microservices",
    "moleculer",
    "PostgreSQL",
    "boilerplate",
    "example",
    "sample"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^7.32.0",
    "moleculer-repl": "^0.6.6"
  },
  "dependencies": {
    "glob": "^8.0.3",
    "moleculer": "^0.14.16",
    "moleculer-web": "^0.10.2",
    "pg": "^8.7.3"
  },
  "engines": {
    "node": ">= 12.x.x"
  }
}
